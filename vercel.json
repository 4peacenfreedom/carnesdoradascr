{
  "buildCommand": "cd react-app && npm run build",
  "outputDirectory": "react-app/dist",
  "devCommand": "cd react-app && npm run dev",
  "installCommand": "cd react-app && npm install --legacy-peer-deps",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
